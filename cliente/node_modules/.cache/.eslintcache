/proyectos/NuevoProyecto.js",["81"],"import React, { useContext, useState } from 'react'\nimport ProyectoContext from '../../context/proyectos/proyectoContext'\n\nconst NuevoProyecto = () => {\n\n    //obtener el state del formulario\n    const proyectosContext = useContext(ProyectoContext)\n    const { formulario, errorFormulario, mostrarFormulario, agregarProyecto, mostrarError } = proyectosContext;\n\n    const [proyecto, guardarProyecto] = useState({\n        nombre: ''\n    })\n\n    const { nombre } = proyecto;\n\n    const onChange = (e) => {\n        guardarProyecto({\n            ...proyecto,\n            [e.target.name]: e.target.value\n        })\n    }\n\n    const onSubmit = (e) => {\n        e.preventDefault();\n\n        if(nombre == ''){\n            mostrarError()\n            return\n        }\n        \n        agregarProyecto(proyecto)\n\n        guardarProyecto({nombre:''})\n\n    }\n\n\n    return (\n        <>\n            <button\n                type='button'\n                className='btn btn-block btn-primario'\n                onClick={()=>mostrarFormulario()}\n            >Nuevo Proyecto</button>\n            {formulario\n                ?\n                    (\n                        <form\n                            className='formulario-nuevo-proyecto'\n                            onSubmit={onSubmit}\n                        >\n                            <input\n                                type=\"text\"\n                                className='input-text'\n                                placeholder='Nombre Proyecto'\n                                name=\"nombre\"\n                                value={nombre}\n                                onChange={onChange}\n                            />\n                            <input\n                                type=\"submit\"\n                                className='btn btn-block btn-primario'\n                                name=\"\"\n                                value=\"Agregar Proyecto\"\n                            />\n                        </form>\n                    )\n                :\n                    null\n            }\n            {errorFormulario\n                ?<p className='mensaje error'>El nombre es obligatorio</p>\n                :null\n            }\n\n        </>\n    )\n}\n\nexport default NuevoProyecto\n","/home/edwin/proyectos/cursos_react/MERN/cliente/src/components/proyectos/ListadoProyectos.js",["82"],"import React, { useContext, useEffect } from 'react'\nimport Proyecto from './Proyecto'\nimport proyectoContext from '../../context/proyectos/proyectoContext'\n\n\n\n\nconst ListadoProyectos = () => {\n\n    //*Extraer proyectos del stateInitial\n    const proyectosContext = useContext(proyectoContext);\n    const {proyectos, obtenerProyectos} = proyectosContext;\n\n    useEffect(() => {\n        obtenerProyectos();\n     }, [])\n\n\n    //*Si no hay proyectos\n    if(proyectos.length === 0) return null;\n\n   \n\n    return (\n        <ul className='listado-proyectos'>\n            {proyectos.map(proyecto => (\n                <Proyecto\n                    key={proyecto.id}\n                    proyecto={proyecto}\n                />\n            ))}\n\n            \n        </ul>\n    )\n}\n\nexport default ListadoProyectos\n","/home/edwin/proyectos/cursos_react/MERN/cliente/src/components/proyectos/Proyecto.js",[],"/home/edwin/proyectos/cursos_react/MERN/cliente/src/components/layout/Barra.js",[],"/home/edwin/proyectos/cursos_react/MERN/cliente/src/components/tareas/FormTarea.js",[],"/home/edwin/proyectos/cursos_react/MERN/cliente/src/components/tareas/ListadoTareas.js",[],"/home/edwin/proyectos/cursos_react/MERN/cliente/src/components/tareas/Tarea.js",[],"/home/edwin/proyectos/cursos_react/MERN/cliente/src/context/proyectos/proyectoState.js",[],"/home/edwin/proyectos/cursos_react/MERN/cliente/src/context/proyectos/proyectoContext.js",["83"],"import { createContext, useContext } from \"react\"\n\nconst proyectoContext = createContext();\nexport default proyectoContext","/home/edwin/proyectos/cursos_react/MERN/cliente/src/context/proyectos/proyectoReducer.js",["84"],"import { AGREGAR_PROYECTO, FORMULARIO_PROYECTO, OBTENER_PROYECTOS, VALIDAR_FORMULARIO, PROYECTO_ACTUAL } from \"../../types\"\n\n\nexport default (state, action) =>{\n    switch(action.type){\n        case FORMULARIO_PROYECTO:\n            return{\n                ...state,\n                formulario:true\n            }\n        case OBTENER_PROYECTOS:\n            return{\n                ...state,\n                proyectos: action.payload\n            }\n        case AGREGAR_PROYECTO:\n            return{\n                ...state,\n                proyectos:[...state.proyectos, action.payload],\n                formulario:false,\n                errorFormulario: false\n            }\n        case VALIDAR_FORMULARIO:\n            return{\n                ...state,\n                errorFormulario: true\n            }\n        case PROYECTO_ACTUAL:\n            return{\n                ...state,\n                proyecto: state.proyectos.filter(proyecto => proyecto.id === action.payload)\n            }\n\n\n        default:\n        return action.type\n    }\n}","/home/edwin/proyectos/cursos_react/MERN/cliente/src/types/index.js",[],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":26,"column":19,"nodeType":"91","messageId":"92","endLine":26,"endColumn":21},{"ruleId":"93","severity":1,"message":"94","line":16,"column":9,"nodeType":"95","endLine":16,"endColumn":11,"suggestions":"96"},{"ruleId":"97","severity":1,"message":"98","line":1,"column":25,"nodeType":"99","messageId":"100","endLine":1,"endColumn":35},{"ruleId":"101","severity":1,"message":"102","line":4,"column":1,"nodeType":"103","endLine":38,"endColumn":2},"no-native-reassign",["104"],"no-negated-in-lhs",["105"],"eqeqeq","Expected '===' and instead saw '=='."